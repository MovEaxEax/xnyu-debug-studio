main
{
	float posX = 430.0;
	float posY = -15550.0;
	float posZ = 1200.0;

	while(Player.Position-Y > -15600.0)
	{
		Player.SetPosition(posX, posY, 2000.0);
		repeat(3)
		{
			Player.SetPosition(posX, posY, posZ);
			frame{}
		}

		posX += 0.75;
		
		repeat(2)
		{
			repeat(10)
			{
				frame { JOYUP(); }
			}
			repeat(10)
			{
				frame { JOYY(); }
			}
			repeat(190)
			{
				frame{}
				if(Player.Position-Y < -15580.0)
				{
					log("FINDING: ", posX);
					Exit();
				}
			}
		}
	}

}

